{
  "$id": "settings.v1alpha1.Endpoint.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Endpoint connects: listener -\u003e routes -\u003e apps",
  "patternProperties": {
    "^(listenerId)$": {
      "type": "string"
    }
  },
  "properties": {
    "id": {
      "type": "string"
    },
    "listener_id": {
      "type": "string"
    },
    "middlewares": {
      "items": {
        "$ref": "settings.v1alpha1.middleware.v1.Middleware.schema.json"
      },
      "type": "array"
    },
    "routes": {
      "items": {
        "$ref": "settings.v1alpha1.Route.schema.json"
      },
      "type": "array"
    }
  },
  "title": "Endpoint",
  "type": "object"
}
