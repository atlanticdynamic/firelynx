{
  "$id": "settings.v1alpha1.ConfigTransaction.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "ConfigTransaction represents an attempt to load a config, stored as a full transaction with details.",
  "patternProperties": {
    "^(created_at)$": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "^(is_valid)$": {
      "type": "boolean"
    },
    "^(request_id)$": {
      "type": "string"
    },
    "^(source_detail)$": {
      "type": "string"
    }
  },
  "properties": {
    "createdAt": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "id": {
      "type": "string"
    },
    "isValid": {
      "type": "boolean"
    },
    "logs": {
      "items": {
        "$ref": "settings.v1alpha1.LogRecord.jsonschema.json"
      },
      "type": "array"
    },
    "requestId": {
      "type": "string"
    },
    "source": {
      "anyOf": [
        {
          "enum": [
            "SOURCE_UNSPECIFIED",
            "SOURCE_FILE",
            "SOURCE_API",
            "SOURCE_TEST"
          ],
          "title": "Source",
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ]
    },
    "sourceDetail": {
      "type": "string"
    },
    "state": {
      "type": "string"
    }
  },
  "title": "Config Transaction",
  "type": "object"
}
